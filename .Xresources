!disable scrollbar
urxvt.scrollBar: false

! font
*.font:         xft:terminus:size=9

! special
*.foreground:   #dadada
*.background:   #1b1b1b
*.cursorColor:  #dadada

! black
*.color0:       #3d3d3d
*.color8:       #4d4d4d

! red
*.color1:       #6673bf
*.color9:       #899aff

! green
*.color2:       #3ea290
*.color10:      #52ad91

! yellow
*.color3:       #b0ead9
*.color11:      #98c9bb

! blue
*.color4:       #31658c
*.color12:      #477ab3

! magenta
*.color5:       #596196
*.color13:      #7882bf

! cyan
*.color6:       #8292b2
*.color14:      #95a7cc

! white
*.color7:       #c8cacc
*.color15:      #e4e4e4

!C-u C-d for scrolling
!URxvt.keysym.C-u: command:\033]720;7\007
!URxvt.keysym.C-d: command:\033]721;7\007

!Activate urls

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
